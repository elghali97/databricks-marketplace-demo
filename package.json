{
  "name": "databricks-marketplace-demo",
  "version": "1.0.0",
  "description": "Full-stack Databricks Financial Data Marketplace",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "cd client && npm run dev",
    "dev:backend": "cd server && ./venv/bin/python scripts/dev.py",
    "build": "vite build",
    "build:frontend": "cd client && npm run build",
    "start": "python server/scripts/prod.py",
    "start:prod": "npm run build && npm run start",
    "install:all": "npm install && cd client && npm install && cd ../server && python3 setup.py",
    "clean": "cd client && rm -rf dist node_modules && cd ../server && rm -rf __pycache__ && rm -rf dist",
    "test:frontend": "cd client && npm run test",
    "lint:frontend": "cd client && npm run lint",
    "docker:dev": "docker-compose --profile dev up --build",
    "docker:prod": "docker-compose --profile prod up --build",
    "docker:full": "docker-compose --profile full up --build"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "clsx": "^2.1.0",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.22.3",
    "recharts": "^2.12.1",
    "tailwind-merge": "^2.2.1",
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "concurrently": "^8.2.2"
  },
  "devDependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/databricks-marketplace-demo.git"
  },
  "keywords": [
    "databricks",
    "marketplace",
    "financial-data",
    "react",
    "fastapi",
    "typescript",
    "python"
  ],
  "author": "Your Organization",
  "license": "MIT"
} 